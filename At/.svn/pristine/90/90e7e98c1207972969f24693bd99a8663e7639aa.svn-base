<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ylmall.at.dao.StorkChangeDao">

	<insert id="insertStorkChange" parameterType="com.ylmall.at.tobject.ResultShop">
	    insert into storkchange(shopCode,sku,changes,time)
		values
		<foreach collection="childOrderList" item="childItem" separator=",">		
			<foreach collection="childItem.infoList" item="Item" separator=",">		
			(#{childItem.shopCode},#{Item.sku},#{Item.amount},#{childItem.time})
			</foreach>
		</foreach>
	</insert>
</mapper>